{
    "name": "Youtube to MP3",
    "description": "Convert Youtube videos to MP3",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["tabs" , "downloads"],
    "action": {
        "default_popup": "popup.html"
      },
    "icons": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "128": "/images/icon128.png"
    }
      
  }