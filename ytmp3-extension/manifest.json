{
    "name": "Youtube to MP3",
    "description": "Convert Youtube videos to MP3",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["tabs"],
    "action": {
        "default_popup": "popup.html"
      }
      
  }